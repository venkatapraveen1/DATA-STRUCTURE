#include <stdio.h>

int main() {
    int array[5] = {1, 2, 3, 4, 5};
    int size = 5;
    int position = 2;
    int newValue = 10;

    for (int i = size - 1; i >= position; i--) {
        array[i + 1] = array[i];
    }

    array[position] = newValue;
    size++;
    printf("192373023\n");
    printf("Array after insertion:\n");
    for (int i = 0; i < size; i++) {
        printf("%d ", array[i]);
    }

    return 0;
}
