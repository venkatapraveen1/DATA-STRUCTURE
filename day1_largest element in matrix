#include <stdio.h>

int main() {
    int matrix[3][3] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
    int max_element = matrix[0][0];

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (matrix[i][j] > max_element) {
                max_element = matrix[i][j];
            }
        }
    }
    printf("reg no=192373023");
    printf("The largest element in the matrix is: %d\n", max_element);

    return 0;
}
